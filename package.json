{
  "name": "@crystal-ball/semantic-release-base",
  "version": "2.34.0",
  "description": "ðŸ”® Projects - Semantic Release configs",
  "main": "index.js",
  "keywords": [
    "crystal-ball",
    "semantic-release"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/semantic-release-base.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "scripts": {
    "commit": "cz",
    "test": "npm run test:lint && npm run test:unit",
